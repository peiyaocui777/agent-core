    "test:v3": "tsx src/test-v3.ts",
    "test:v4": "tsx src/test-v4.ts",
    "test:v5": "tsx src/test-v5.ts",
    "test:v6": "tsx src/test-v6.ts",
    "test:v7": "tsx src/test-v7.ts",
    "test:v8": "tsx src/test-v8.ts",
    "test:v9": "tsx src/test-v9.ts",
    "test:llm": "tsx src/test-llm-e2e.ts",
    "init": "tsx src/cli/bin.ts init",
    "jarvis": "tsx src/cli/bin.ts"
  },
  "keywords": [
