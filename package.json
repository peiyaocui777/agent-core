    "start:cli": "tsx src/cli/bin.ts chat",
    "start:telegram": "tsx src/main.ts --telegram",
    "start:mcp": "tsx src/mcp/start-server.ts",
    "start:dashboard": "tsx src/cli/bin.ts dashboard",
    "start:marketplace": "tsx src/cli/bin.ts marketplace",
    "start:web": "tsx src/cli/bin.ts web",
