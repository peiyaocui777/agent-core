  "author": "xuyuyu",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "jarvis.config.yaml",
    ".env.example"
  ],
  "optionalDependencies": {
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
